<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cpacm.demo.testrep" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".mainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TweenAnimationActivity"
            android:label="@string/title_activity_main" >
        </activity>
        <activity
            android:name=".FrameAnimationActivity"
            android:label="@string/title_activity_frame_animation" >
        </activity>
        <activity
            android:name=".ObjectAnimatorActivity"
            android:label="@string/title_activity_object_animator" >
        </activity>
    </application>

</manifest>
